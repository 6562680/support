{
    "name": "gzhegow/support",
    "description": "Support libraries",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Gzhegow\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gzhegow\\Support\\Tests\\": "tests/src"
        }
    }
}
