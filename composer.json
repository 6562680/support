  "require": {
    "php": ">=7.3",
    
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    
    "psr/container": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    
    "symfony/var-dumper": "~5.0",
    "symfony/string": "^5.3",
    "symfony/translation-contracts": "^2.4",
    
    "nette/php-generator": "^3.5",
    "nikic/php-parser": "^4.10",
    "doctrine/inflector": "^2.0"
  },
  
  "autoload": {
    "psr-4": {
      "Gzhegow\\Support\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gzhegow\\Support\\Tests\\": "tests/src"
    }
