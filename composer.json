  "require-dev": {
    "ext-mbstring": "*",
    
    "phpunit/phpunit": "^9.0",
    
    "psr/cache": "^1.0",
    "psr/container": "^2.0",
    "psr/log": "^1.1",
    
    "doctrine/inflector": "^2.0",
    
