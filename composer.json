    "nikic/php-parser": "^4.0"
  },
  
  "autoload": {
    "psr-4": {
      "Gzhegow\\Support\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gzhegow\\Support\\Tests\\": "tests/src"
